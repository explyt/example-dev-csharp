{
  "openapi": "3.0.1",
  "info": {
    "title": "PricingService",
    "version": "1.0"
  },
  "paths": {
    "/api/Pricing": {
      "post": {
        "tags": [
          "Pricing"
        ],
        "requestBody": {
          "content": {
            "application/json-patch+json": {
              "schema": {
                "$ref": "#/components/schemas/CalculatePriceCommand"
              }
            },
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CalculatePriceCommand"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CalculatePriceCommand"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CalculatePriceCommand"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CalculatePriceCommand": {
        "type": "object",
        "properties": {
          "productCode": {
            "type": "string",
            "nullable": true
          },
          "policyFrom": {
            "type": "string",
            "format": "date-time"
          },
          "policyTo": {
            "type": "string",
            "format": "date-time"
          },
          "selectedCovers": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "answers": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/QuestionAnswer"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "QuestionAnswer": {
        "type": "object",
        "properties": {
          "questionCode": {
            "type": "string",
            "nullable": true
          },
          "questionType": {
            "$ref": "#/components/schemas/QuestionType"
          }
        },
        "additionalProperties": false
      },
      "QuestionType": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      }
    }
  }
}